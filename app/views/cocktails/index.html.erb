<div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(data:image/jpeg;base64,)">
    <h1>List of cocktails</h1>
</div>
<div class="container">
  <ul class="list-unstyled">
    <% @cocktails.each do |cocktail| %>
      <div class="card-product">
        <img src="https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/skateboard.jpg" />
        <div class="card-product-infos">
          <li><%= link_to cocktail.name, cocktail_path(cocktail) %></li>
        </div>
      </div>
    <% end %>
  </ul>
  <hr>
  <%= link_to "Create your own cocktail", new_cocktail_path %> |
</div>
